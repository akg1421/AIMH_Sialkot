
<div class="container">
    <div class="row">
        @*<p><small>Incase you delete yourself contact pitb.</small></p>*@
        <table class="table table-striped table-bordered">

            @if (ViewBag.AdminUserList != null)
            {
                <tr>
                    <th class="col-md-12">
                        Name
                    </th>
                    @*<th class="col-md-1">
                    </th>*@
                </tr>
            }

            @foreach (var x in ViewBag.AdminUserList)
            {
                <tr>
                    <td>
                        @x.UserName
                    </td>
                    @*<td>
                        <div class="btn-group btn-group-xs" role="group" aria-label="...">
                            <a href='@Url.Action("Delete", "Users", new { userid = x.Id })' title="Delete" target="_blank" class="btn btn-default btn-xs"><span class="glyphicon glyphicon-remove"></span></a>
                        </div>
                    </td>*@
                </tr>
            }

            @foreach (var y in ViewBag.OptUserList)
            {
                <tr>
                    <td>
                        @y.UserName
                    </td>
                    @*<td>
                        <div class="btn-group btn-group-xs" role="group" aria-label="...">
                            <a href='@Url.Action("Delete", "Users", new { userid = y.Id })' title="Delete" target="_blank" class="btn btn-default btn-xs"><span class="glyphicon glyphicon-remove"></span></a>
                        </div>
                    </td>*@
                </tr>
            }
        </table>
    </div>
</div>